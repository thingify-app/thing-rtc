{
  "compilerOptions": {
    "module": "ES6",
    "target": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types"],
    "outDir": "dist"
  },
  "include": [
    "./src/*.ts",
    "./src/**/*.ts",
    "./node_modules/@cloudflare/workers-types/index.d.ts"
  ],
  "exclude": ["node_modules/", "dist/"]
}
  